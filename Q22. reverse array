#include <stdio.h>

int main() {
    int n;
    printf("Enter the size of array: ");
    scanf("%d", &n);
    int arr[n];
    int rvv[n];
    for(int i = 0; i < n; i++) {
        printf("Enter element %d: ", i + 1);
        scanf("%d", &arr[i]);
    }
    
    for(int i = 0; i < n; i++) {
        rvv[i] = arr[n - 1 - i];
        printf("%d ", rvv[i]);}
    return 0;
}
