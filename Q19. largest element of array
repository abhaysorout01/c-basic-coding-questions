#include <stdio.h>

int main() {
    int n;
    int largest = -2147483648;
    printf("Enter the size of array: ");
    scanf("%d", &n);
    int arr[n];
    for(int i = 0; i < n; i++) {
        printf("Enter element %d: ", i + 1);
        scanf("%d", &arr[i]);
    }
    for(int i = 0; i < n; i++) {
        if(arr[i] > largest) {
            largest = arr[i];
        }
    }
    printf("The largest element is: %d\n", largest);
    return 0;
}
