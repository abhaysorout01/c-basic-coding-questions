#include<stdio.h>
int main() {
    char a;
    printf("Enter the alphabet:");
    scanf("%d", &a);
    if((a >= 'a' && a <= 'z') || (a >= 'A' && a == 'Z')) { 
        if(a == 'a' || a == 'e' || a == 'i' || a == 'o' || a == 'u' || a == 'A' || a == 'E' || a == 'I' || a == 'O' || a == 'U') {
        printf("Vowel");
    } else {
        printf("constant");
    }
    } else {
        printf("Enter only a alphabet");
    }
    
    return 0;
}
