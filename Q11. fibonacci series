#include<stdio.h>
int main() {
    int a = 0;
    int b = 1;
    int n;
    int c = 0;
    printf("Enter the number:");
    scanf("%d", &n);
    if(n > 0) {
        if(n == 1) {
        printf("%d ", a);
    } else if(n == 2) {
        printf("%d %d ", a, b);
    } else {
        printf("%d ", a);
        while(n>0) {
            a = b;
            b = c;
            c = a + b;
            printf("%d ", c);
            n--;
        }
    }
} else {
    printf("Invalid number.");
}
    return 0;
}
