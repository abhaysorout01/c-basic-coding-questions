#include <stdio.h>

int main() {
    int n;
    printf("Enter the size of array: ");
    scanf("%d", &n);
    int arr[n];
    for(int i = 0; i < n; i++) {
        printf("Enter element %d: ", i + 1);
        scanf("%d", &arr[i]);
    }
    int search;
    printf("Enter the element to search: ");
    scanf("%d", &search);
    int found = 0;
    for(int i = 0; i < n; i++) {
        if(arr[i] == search) {
            printf("Element found at index %d\n", i+1);
            break;
        }
    }
    return 0;
}
