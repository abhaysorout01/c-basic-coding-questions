#include <stdio.h>
int main() {
    int n,a,b,i;
    printf("Enter the size of array: ");
    scanf("%d", &n);
    int arr[n];
    for(i = 0; i < n; i++) {
        printf("Enter element %d: ", i + 1);
        scanf("%d", &arr[i]);
    }
    printf("Enter the number you want to delete:");
    scanf("%d", &a);
    printf("Your new array is:");
    for(int i = 0;i < n;i++) {
        if(arr[i] != a) {
            printf("%d ", arr[i]);
        }
    }
    return 0;
}
