#include <stdio.h>

int main() {
    int n;
    float sum = 0;
    printf("Enter the number of elements: ");
    scanf("%d", &n);
    int arr[n];
    for(int i = 0; i < n; i++) {
        printf("Enter element %d: ", i + 1);
        scanf("%d", &arr[i]);
    }
    for(int i = 0; i < n; i++) {
        sum += arr[i];
    }
    printf("The average of your elements is: %f\n", sum/n);
    return 0;
}
