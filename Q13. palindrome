#include<stdio.h>
int main() {
    int a;
    int d = 0;
    printf("Enter the number: ");
    scanf("%d", &a);
    int c = a;
    while(a > 0) {
        d = d*10 + a % 10;
        a = a / 10;
    }
    if(c == d) {
        printf("The number is a palindrome.");
    } else {
        printf("The number is not a palindrome.");
    }
    return 0;
}
